{
  "name": "@addax/layout",
  "version": "1.1.0",
  "license": "MIT",
  "author": "aiiua",
  "description": "a admin layout",
  "main": "lib/addax.umd.min.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aiiua/addax-layout.git"
  },
  "keywords": [
    "vue",
    "iview",
    "admin",
    "template",
    "layout"
  ],
  "bugs": {
    "url": "https://github.com/aiiua/addax-layout/issues"
  },
  "homepage": "https://github.com/aiiua/addax-layout#readme",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lib": "vue-cli-service build --target lib --name addax --dest lib src/components/index.js",
    "lint": "vue-cli-service lint",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "commit": "git-cz",
    "lint:style": "vue-cli-service lint:style",
    "push": "node commitlint.script.js",
    "release": "standard-version"
  },
  "dependencies": {
    "view-design": "^4.1.3",
    "vue": "^2.6.11",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "babel-plugin-component": "^1.1.1",
    "@ascendancyy/vue-cli-plugin-stylelint": "~1.1.2",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@gfx/zopfli": "^1.0.14",
    "@vue/cli-plugin-babel": "~4.2.3",
    "@vue/cli-plugin-eslint": "~4.2.3",
    "@vue/cli-plugin-vuex": "~4.2.3",
    "@vue/cli-service": "~4.2.3",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "brotli-webpack-plugin": "^1.1.0",
    "commitizen": "^4.0.3",
    "compression-webpack-plugin": "^3.1.0",
    "conventional-changelog": "^3.1.18",
    "conventional-changelog-cli": "^2.0.31",
    "core-js": "^3.6.4",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.3",
    "inquirer": "^7.1.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.0.8",
    "shelljs": "^0.8.3",
    "standard-version": "^7.1.0",
    "stylelint-config-standard": "^20.0.0",
    "vue-cli-plugin-commitlint-release": "^1.1.4",
    "vue-cli-plugin-iview": "^2.0.0",
    "vue-template-compiler": "^2.6.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint"
    ],
    "**/*.less": "stylelint --syntax less"
  }
}
